msgid "{dans} days "
msgstr "{dans} dagar"

#: cogs/experience_related_commands.py:95 cogs/scores.py:36
msgid "{dans} hours"
msgstr "{dans} timmar"

#: cogs/experience_related_commands.py:95 cogs/scores.py:37
